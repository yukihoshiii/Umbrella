function main(): number {
    println("Test: Map Literals");
    let user = {
        "name": "Yuki",
        "role": "Admin",
        "active": "true"
    };
    
    println("User Name: " + user.get("name"));
    println("User Role: " + user.get("role"));
    
    user.set("status", "Offline");
    println("User Status: " + user.get("status"));
    
    return 0;
}
