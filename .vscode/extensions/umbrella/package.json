{
    "name": "umbrella",
    "displayName": "Umbrella Language Support",
    "description": "Syntax highlighting for Umbrella programming language",
    "version": "1.0.0",
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "umbrella",
                "aliases": [
                    "Umbrella",
                    "umbrella"
                ],
                "extensions": [
                    ".umb"
                ],
                "configuration": "./language-configuration.json",
                "icon": {
                    "light": "./umbrella-icon-light.svg",
                    "dark": "./umbrella-icon-dark.svg"
                }
            }
        ],
        "grammars": [
            {
                "language": "umbrella",
                "scopeName": "source.umbrella",
                "path": "./syntaxes/umbrella.tmLanguage.json"
            }
        ]
    }
}